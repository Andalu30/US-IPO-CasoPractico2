<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout
        xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:tools="http://schemas.android.com/tools"
        xmlns:app="http://schemas.android.com/apk/res-auto"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        tools:context=".resumeninfo">

    <TextView
            android:text="Revise sus datos"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content" app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintStart_toStartOf="parent" android:layout_marginTop="16dp"
            app:layout_constraintEnd_toEndOf="parent" android:id="@+id/textView9"
            android:textAppearance="@android:style/TextAppearance.Material.Headline"/>
    <TextView
            android:text="DNI"
            android:layout_width="71dp"
            android:layout_height="25dp"
            android:id="@+id/textView10"
            android:textAppearance="@android:style/TextAppearance.Material.Medium"
            app:layout_constraintEnd_toStartOf="@+id/editText5" android:layout_marginEnd="8dp"
            app:layout_constraintStart_toStartOf="parent" android:layout_marginStart="8dp"
            app:layout_constraintHorizontal_bias="0.95" android:layout_marginTop="32dp"
            app:layout_constraintTop_toBottomOf="@+id/textView9"/>
    <TextView
            android:text="Fecha"
            android:layout_width="71dp"
            android:layout_height="25dp"
            android:id="@+id/textView11"
            android:textAppearance="@android:style/TextAppearance.Material.Medium"
            app:layout_constraintStart_toStartOf="parent" android:layout_marginStart="100dp"
            app:layout_constraintEnd_toStartOf="@+id/editText7" android:layout_marginEnd="8dp"
            android:layout_marginTop="28dp" app:layout_constraintTop_toBottomOf="@+id/textView12"
            app:layout_constraintHorizontal_bias="0.7"/>
    <TextView
            android:text="Hora"
            android:layout_width="71dp"
            android:layout_height="25dp"
            android:id="@+id/textView13"
            android:textAppearance="@android:style/TextAppearance.Material.Medium"
            app:layout_constraintStart_toStartOf="parent" android:layout_marginStart="104dp"
            app:layout_constraintEnd_toStartOf="@+id/editText8" android:layout_marginEnd="8dp"
            android:layout_marginTop="32dp" app:layout_constraintTop_toBottomOf="@+id/textView11"/>
    <TextView
            android:text="Comisaria"
            android:layout_width="92dp"
            android:layout_height="28dp"
            android:id="@+id/textView12"
            android:textAppearance="@android:style/TextAppearance.Material.Medium"
            app:layout_constraintEnd_toStartOf="@+id/editText6" android:layout_marginEnd="8dp"
            app:layout_constraintStart_toStartOf="parent" android:layout_marginStart="8dp"
            app:layout_constraintHorizontal_bias="1.0" android:layout_marginTop="44dp"
            app:layout_constraintTop_toBottomOf="@+id/textView10"/>
    <EditText
            android:layout_width="114dp"
            android:layout_height="43dp"
            android:ems="10"
            android:id="@+id/editText5" android:text="12345678A"
            app:layout_constraintEnd_toEndOf="parent"
            android:layout_marginEnd="108dp"
            android:layout_marginTop="32dp" app:layout_constraintTop_toBottomOf="@+id/textView9"/>
    <EditText
            android:layout_width="118dp"
            android:layout_height="44dp"
            android:ems="10"
            android:id="@+id/editText6" android:text="Comisaria 1"
            app:layout_constraintEnd_toEndOf="parent"
            android:layout_marginEnd="108dp" android:layout_marginTop="24dp"
            app:layout_constraintTop_toBottomOf="@+id/editText5"/>
    <EditText
            android:layout_width="114dp"
            android:layout_height="wrap_content"
            android:ems="10"
            android:id="@+id/editText7" android:text="01/01/01"
            app:layout_constraintEnd_toEndOf="parent"
            android:layout_marginEnd="108dp" android:layout_marginTop="8dp"
            app:layout_constraintTop_toBottomOf="@+id/editText6"/>
    <EditText
            android:layout_width="114dp"
            android:layout_height="39dp"
            android:ems="10"
            android:id="@+id/editText8" android:text="9:00"
            android:layout_marginTop="20dp" app:layout_constraintTop_toBottomOf="@+id/editText7"
            android:layout_marginEnd="108dp" app:layout_constraintEnd_toEndOf="parent"/>
    <Button
            android:text="Confirmar"
            android:layout_width="193dp"
            android:layout_height="60dp"
            app:layout_constraintStart_toStartOf="parent" app:layout_constraintEnd_toEndOf="parent"
            android:id="@+id/confbutt" style="@android:style/Widget.Material.Light.Button"
            android:backgroundTint="?android:attr/colorActivatedHighlight"
            android:textAppearance="@android:style/TextAppearance.Material.Large" android:layout_marginBottom="8dp"
            app:layout_constraintBottom_toTopOf="@+id/atrasresumen"/>
    <Button
            android:text="Atras"
            android:layout_width="193dp"
            android:layout_height="60dp"
            app:layout_constraintStart_toStartOf="parent" app:layout_constraintEnd_toEndOf="parent"
            android:id="@+id/atrasresumen" style="@android:style/Widget.Material.Light.Button"
            android:backgroundTint="?android:attr/colorPrimary"
            android:textAppearance="@android:style/TextAppearance.Material.Large" android:layout_marginBottom="8dp"
            app:layout_constraintBottom_toBottomOf="parent"/>
</android.support.constraint.ConstraintLayout>