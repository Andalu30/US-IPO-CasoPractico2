<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout
        xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:tools="http://schemas.android.com/tools"
        xmlns:app="http://schemas.android.com/apk/res-auto"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        tools:context=".borracita">

    <TextView
            android:text="Mis citas"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/textView16"
            android:textAppearance="@android:style/TextAppearance.Material.Large"
            app:layout_constraintStart_toStartOf="parent" android:layout_marginStart="16dp"
            android:layout_marginTop="16dp" app:layout_constraintTop_toTopOf="parent" android:layout_marginEnd="8dp"
            app:layout_constraintEnd_toEndOf="parent" app:layout_constraintHorizontal_bias="0.0"/>
    <TextView
            android:text="Otras Citas"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/textView17"
            android:textAppearance="@android:style/TextAppearance.Material.Large"
            app:layout_constraintStart_toStartOf="parent" android:layout_marginStart="8dp"
            android:layout_marginEnd="8dp"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.003" android:layout_marginTop="36dp"
            app:layout_constraintTop_toBottomOf="@+id/checkBox6"/>
    <EditText
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:inputType="textPersonName"
            android:ems="10"
            android:id="@+id/editText11"
            android:hint="Localizador de la cita"
            android:layout_marginEnd="8dp"
            app:layout_constraintEnd_toEndOf="parent" android:layout_marginStart="8dp"
            app:layout_constraintStart_toStartOf="parent" android:layout_marginTop="4dp"
            app:layout_constraintTop_toBottomOf="@+id/checkBox7" app:layout_constraintHorizontal_bias="0.395"/>
    <EditText
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:inputType="textPersonName"
            android:ems="10"
            android:id="@+id/editText12"
            android:hint="DNI"
            android:layout_marginEnd="8dp"
            app:layout_constraintEnd_toEndOf="parent" android:layout_marginStart="8dp"
            app:layout_constraintStart_toStartOf="parent" app:layout_constraintHorizontal_bias="0.434"
            android:layout_marginTop="8dp" app:layout_constraintTop_toBottomOf="@+id/checkBox8"/>
    <Button
            android:text="Anular Citas"
            android:layout_width="174dp"
            android:layout_height="55dp"
            app:layout_constraintStart_toStartOf="parent" app:layout_constraintEnd_toEndOf="parent"
            android:id="@+id/anulabutt" android:layout_marginBottom="8dp"
            app:layout_constraintBottom_toBottomOf="parent" android:backgroundTint="@color/colorAccent"
            android:textAppearance="@android:style/TextAppearance.Material.Widget.Button.Colored"/>
    <CheckBox
            android:text="Cita 1 (Nombre, Falso. DNI. Sevilla centro)"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            tools:layout_editor_absoluteX="20dp" android:id="@+id/checkBox3" android:layout_marginTop="12dp"
            app:layout_constraintTop_toBottomOf="@+id/textView16"/>
    <CheckBox
            android:text="Cita 2 (Joseph,Ignatius. DNIyPASS. Sevilla Triana)"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            tools:layout_editor_absoluteX="20dp" android:id="@+id/checkBox5"
            android:layout_marginTop="8dp" app:layout_constraintTop_toBottomOf="@+id/checkBox3"/>
    <CheckBox
            android:text="Cita 3 (Krovas, Dagger. NIE. AlmerÃ­a)"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            tools:layout_editor_absoluteX="20dp" android:id="@+id/checkBox6"
            android:layout_marginTop="8dp" app:layout_constraintTop_toBottomOf="@+id/checkBox5"/>
    <CheckBox
            android:text="Anular cita mediante localizador"
            android:layout_width="wrap_content"
            android:layout_height="30dp"
            android:id="@+id/checkBox7" android:layout_marginEnd="8dp" app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent" android:layout_marginStart="8dp"
            app:layout_constraintHorizontal_bias="0.074" android:layout_marginTop="8dp"
            app:layout_constraintTop_toBottomOf="@+id/textView17"/>
    <CheckBox
            android:text="Anular cita mediante DNI"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/checkBox8" android:layout_marginEnd="8dp" app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent" android:layout_marginStart="8dp"
            app:layout_constraintHorizontal_bias="0.057" android:layout_marginTop="24dp"
            app:layout_constraintTop_toBottomOf="@+id/editText11"/>
</android.support.constraint.ConstraintLayout>